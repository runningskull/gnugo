Pattern RD000a

X*

:8,D,minvalue(0),maxvalue(98)

a*

; rgoal[a] == 1

> {
> int ostar = olib(*);
> int xstar = xlib(*);
> if (lib(a) == 2 && xstar > 2 && xstar > ostar)
>   value = 75;
> else if (lib(a) == 2 && xstar > 2)
>   value = 70;
> else if (lib(a) > xstar)
>   value = 0;
> else
>   value = 50;
>}


